@fragrance = variable[1]
@found_fragrance = false

def rub_vial
	fput "rub vial"
end

def analyze_vial
	fput "analyze vial"
	match = matchtimeout 1, @fragrance
	
	if match != false
		@found_fragrance = true
	end
end

if @fragrance.nil?
	echo "You must provide a string from the fragrance you're looking for."
else
	until @found_fragrance == true
		rub_vial
		analyze_vial
	end
end